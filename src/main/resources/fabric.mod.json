{
  "schemaVersion": 1,
  "id": "${id}",
  "version": "${version}",

  "name": "${name}",
  "description": "Template for my fabric mods",
  "authors": [
    "As_One"
  ],
  "contact": {
    "homepage": "https://modrinth.com",
    "sources": "https://github.com/AsOne2020/fabric-mod-template"
  },

  "icon": "assets/template_mod/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.asone.template_mod.TemplateMod"
    ]
  },
  "mixins": [
    "template_mod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.4",
    "minecraft": "${minecraft_dependency}"
  }
}
